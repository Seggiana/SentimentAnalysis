<?import javafx.scene.control.*?>
<?import javafx.scene.control.Tab.*?>


<TabPane fx:id="tabPane" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1"
         fx:controller="sample.RootController" tabClosingPolicy="UNAVAILABLE">
    <tabs>
        <Tab fx:id="tab_welcome" text="Strona główna">
            <fx:include fx:id="fxml_welcome" source="fxml_welcome.fxml"/>
        </Tab>
        <Tab fx:id="tab_learning" text="Metoda słownikowa">
            <fx:include fx:id="fxml_learning" source="fxml_learning.fxml"/>
        </Tab>
        <Tab fx:id="tab_mlm" text="Metody NLP">
            <fx:include fx:id="fxml_mlm" source="fxml_mlm.fxml"/>
        </Tab>
        <Tab fx:id="tab_testing" text="Testowanie">
            <fx:include fx:id="fxml_testing" source="fxml_testing.fxml"/>
        </Tab>
    </tabs>
</TabPane>